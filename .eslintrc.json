{
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "array-callback-return": 2,
    "arrow-parens": [
      2,
      "always"
    ],
    "arrow-spacing": 2,
    "block-spacing": [
      2,
      "never"
    ],
    "brace-style": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "dot-notation": 2,
    "dot-location": [
      2,
      "property"
    ],
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [
      2,
      2
    ],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-len": [
      2,
      128
    ],
    "max-statements-per-line": 2,
    "new-parens": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-console": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-extra-parens": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-label-var": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-multi-spaces": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-prototype-builtins": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-whitespace-before-property": 2,
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "object-curly-spacing": 2,
    "object-shorthand": 2,
    "operator-assignment": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "template-curly-spacing": 2,
    "wrap-iife": 2,
    "yoda": 2
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
